@using Lab2EDI_1C18.Models;
@model  IEnumerable<Lab2EDI_1C18.Models.CargaPais>
@{
    ViewBag.Title = "IndexPre";
}

<h2>IndexPre</h2>

@using (Html.BeginForm("IndexPre", "CargaPais", FormMethod.Post, new { enctype = "multipart/form-data" }))
{

    if (Model != null)
    {
        <br />
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Grupo)
                </th>

                <th>
                    @Html.DisplayNameFor(model => model.nombre)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Grupo)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.nombre)
                    </td>
                </tr>
            }

        </table>
    }
}
