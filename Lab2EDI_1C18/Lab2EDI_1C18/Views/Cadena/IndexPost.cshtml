@model IEnumerable<Lab2EDI_1C18.Models.Cadena>

@{
    ViewBag.Title = "IndexPost";
}

<h2>IndexPost</h2>
<p>
    @Html.ActionLink("Volver", "Index")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Texto)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Texto)
            </td>
            <td>
                @Html.ActionLink("Delete", "Delete", new {  id=item.Texto })
            </td>
        </tr>
    }

</table>
